{
  "expo": {
    "name": "WallCraft",
    "slug": "wcraft",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/2.png",
    "scheme": "wallcraft",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.SET_WALLPAPER",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/2.png",
        "backgroundColor": "#000000"
      },
      "package": "com.tesla254.wcraft"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-admob-native-ads",
        {
          "androidAppId": "ca-app-pub-5482160285556109~4677496359"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "38f26332-e83c-4489-b6f2-e24b906ec9fc"
      }
    }
  }
}
